{
  "name": "ls-template-angular5-v1",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod && ng build --prod --app 1 --output-hashing=false && npm run webpack:server",
    "build:prerender": "npm run build:prod && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:prod && npm run webpack:server",
    "build:local": "ng build --prod --env=local && ng build --prod --env=local --app 1 --output-hashing=false && npm run webpack:server",
    "build:dev": "ng build --prod --env=proddev && ng build --prod --env=proddev --app 1 --output-hashing=false && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "webpack:worker": "webpack --config webpack.worker.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
